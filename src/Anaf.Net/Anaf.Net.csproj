<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://sibies.ro</PackageProjectUrl>
    <Authors>Sibies Soft</Authors>
    <Company>Sibies Soft</Company>
    <Product>Anaf.Net</Product>
    <Copyright>Copyright Â© Sibies Soft</Copyright>
    <RepositoryUrl>https://github.com/sibies/Anaf.Net</RepositoryUrl>
    <PackageTags>anaf servicii spv</PackageTags>
    <NeutralLanguage>ro</NeutralLanguage>
    <PackageIcon>anaf_ro.png</PackageIcon>
    <Version>1.5.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\anaf_ro.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
